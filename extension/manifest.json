{
  "manifest_version": 2,
  "name": "Oberl的区块链钱包",
  "version": "1.0",
  "description": "My first Chrome extension",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "38": "icon38.png",
      "48": "icon48.png",
      
      "128": "icon128.png"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["activeTab", "storage","http://127.0.0.1/*"]
}

